{
    "name": "cyberdas",
    "version": "0.3.0",
    "private": true,
    "dependencies": {
        "@babel/register": "^7.14.5",
        "@brainhubeu/react-carousel": "^2.0.4",
        "@fontsource/quantico": "^4.5.0",
        "@fontsource/roboto": "^4.5.0",
        "@reduxjs/toolkit": "^1.5.1",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.5.0",
        "@testing-library/user-event": "^7.1.2",
        "axios": "^0.21.1",
        "babel-eslint": "^10.1.0",
        "bootstrap": "^5.1.0",
        "date-fns": "^2.23.0",
        "formik": "^2.2.9",
        "is-ci": "^3.0.0",
        "lint-staged": "^11.0.0",
        "next": "^11.1.2",
        "sass": "^1.42.1",
        "postcss": "^8.3.8",
        "prettier": "^2.4.1",
        "prop-types": "^15.7.2",
        "react": "^17.0.2",
        "react-bootstrap": "^2.0.0-beta.5",
        "react-bootstrap-icons": "^1.5.0",
        "react-calendar": "^3.4.0",
        "react-confirm": "^0.1.24",
        "react-dom": "^17.0.2",
        "react-ga": "^3.3.0",
        "react-redux": "^7.2.3",
        "react-yandex-metrika": "^2.6.0",
        "yup": "^0.32.9"
    },
    "lint-staged": {
        "*.{js,jsx}": [
            "npx eslint --fix"
        ],
        "*.{css,scss}": [
            "npx stylelint --fix"
        ]
    },
    "scripts": {
        "dev": "next dev",
        "start": "next start",
        "build": "next build",
        "test": "jest",
        "eject": "next eject",
        "prepare": "is-ci || husky install",
        "lint:code": "eslint --max-warnings 0 --ext js,jsx common modules pages",
        "lint:style": "stylelint {common,modules,pages}/**/*.{css,scss} ",
        "format:code": "eslint --ext js,jsx common modules pages --fix",
        "format:style": "stylelint {common,modules,pages}/*/*/*.{css,scss} --fix",
        "pre-commit": "lint-staged",
        "storybook": "start-storybook -p 3001 -s public",
        "storybook:ci": "start-storybook -p 3001 -s public --ci",
        "build-storybook": "build-storybook -s public"
    },
    "eslintConfig": {
        "extends": "react-app",
        "overrides": [
            {
                "files": [
                    "**/*.stories.*"
                ],
                "rules": {
                    "import/no-anonymous-default-export": "off"
                }
            }
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@next/eslint-plugin-next": "^11.1.2",
        "@storybook/addon-actions": "^6.3.7",
        "@storybook/addon-essentials": "^6.3.7",
        "@storybook/addon-links": "^6.3.7",
        "@storybook/node-logger": "^6.3.7",
        "@storybook/react": "^6.3.7",
        "babel-jest": "^27.2.2",
        "css-loader": "^4.3.0",
        "eslint": "^7.32.0",
        "eslint-config-airbnb": "18.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-jsconfig": "^1.1.0",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-jest": "^24.4.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-testing-library": "^4.12.4",
        "husky": "^7.0.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^27.2.2",
        "react-test-renderer": "^17.0.2",
        "sass-loader": "^10.2.0",
        "style-loader": "^2.0.0",
        "stylelint": "^13.13.1",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-standard": "^22.0.0",
        "stylelint-prettier": "^1.2.0",
        "stylelint-scss": "^3.20.1"
    }
}
