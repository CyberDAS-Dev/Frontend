{
    "name": "CyberDAS",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@reduxjs/toolkit": "^1.5.1",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@testing-library/user-event": "^7.1.2",
        "axios": "^0.21.1",
        "babel-eslint": "^10.1.0",
        "bootstrap": "4.6.0",
        "husky": "^7.0.1",
        "lint-staged": "^11.0.0",
        "prettier": "^2.3.2",
        "react": "^17.0.2",
        "react-bootstrap": "^1.6.1",
        "react-dom": "^17.0.2",
